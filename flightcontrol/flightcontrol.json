{
  "environments": [
    {
      "id": "development",
      "name": "Development",
      "region": "us-east-1",
      "source": {
        "branch": "main"
      },
      "services": [
        {
          "id": "redwood-api",
          "name": "Redwood API",
          "type": "fargate",
          "cpu": 0.25,
          "memory": 0.5,
          "port": 8911,
          "installCommand: "cd flightcontrol && yarn install --frozen-lockfile --production=false",
          "buildCommand": "cd flightcontrol && yarn rw deploy flightcontrol",
          "startCommand": "cd flightcontrol && yarn rw deploy flightcontrol --serve",
          "postBuildCommand": "echo 0",
          "healthCheckPath": "/graphql/health"
        },
        {
          "id": "redwood-web",
          "name": "Redwood Web",
          "type": "static",
          "singlePageApp": true,
          "buildCommand": "yarn rw deploy flightcontrol web",
          "outputDirectory": "web/dist"
        }
      ]
    }
  ]
}
